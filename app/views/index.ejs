<div class="col s12">
    <ul class="collapsible xethru_container z-depth-0" data-collapsible="accordion">
    <li ng-repeat = "cell in scan_results | orderBy:orderScanResults:true"
                 ng-class = "{ selected: (cell.mac === selected_cell.mac) }"
                 ng-click = "change_selection(cell)"> <!---->
      <div class="collapsible-header">{{ cell.ssid }}</div>
      <div class="collapsible-body">
        <div class="row">
        <div class="col s7"><span class="details">MAC: {{ cell.mac }}</span>
  
        <input type = 'password' placeholder = 'Password' ng-model ="password" ng-keypress = "(($event.keyCode === 13) && (password !== undefined) )? submit_selection(password) : console.log(test)" />
        <a class="waves-effect waves-light btn xethru_btn " ng-click='submit_selection(password)'>Connect</a>

        </div></div>
      </div>
    </li>
